            {
              "id": "SNYK-ALPINE313-FREETYPE-2809176",
              "shortDescription": {
                "text": "Critical severity - Out-of-bounds Write vulnerability in freetype"
              },
              "fullDescription": {
                "text": "(CVE-2022-27404) freetype/freetype@2.10.4-r1"
              },
